{
  "name": "agent-reachout",
  "version": "1.0.0",
  "description": "Multi-provider notification plugin for Claude Code",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "prestart": "bun install --silent",
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "typecheck": "tsc --noEmit",
    "telegram-agent": "bun run src/telegram-agent.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node-telegram-bot-api": "^0.64.13"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "node-telegram-bot-api": "^0.67.0"
  }
}
